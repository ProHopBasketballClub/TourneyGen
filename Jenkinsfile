  agent any
  stages {
    stage('Build') {
      steps {
        echo 'It\'s happening!!'
      }
    }
    stage('Test') {
      steps {
        echo 'We probably won\'t have time for these'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Woah, wait, what? this thing exists?'
      }
    }
  }
}