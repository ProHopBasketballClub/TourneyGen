{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon": true,
        "ordered-imports": true,
        "eofline": true,
        "no-magic-numbers": true,
        "whitespace": true,
        "no-console": false,
      "variable-name": {
        "options": ["allow-leading-underscore","allow-pascal-case","allow-snake-case","check-format"]
      }
    },
    "rulesDirectory": []
}
